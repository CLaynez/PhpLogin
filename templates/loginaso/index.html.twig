{% extends 'base.html.twig' %}

{% block title %}Loginaso index{% endblock %}

{% block body %}
    <h1>Loginaso index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Email</th>
                <th>Password</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for loginaso in loginasos %}
            <tr>
                <td>{{ loginaso.id }}</td>
                <td>{{ loginaso.email }}</td>
                <td>{{ loginaso.password }}</td>
                <td>
                    <a href="{{ path('app_loginaso_show', {'id': loginaso.id}) }}">show</a>
                    <a href="{{ path('app_loginaso_edit', {'id': loginaso.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_loginaso_new') }}">Create new</a>
{% endblock %}
